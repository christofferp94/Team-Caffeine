<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projektarbetequiz.FXMLChangeEmailController">
   <children>
      <Text layoutX="236.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Change Email">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Text>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#e4e4e4" height="228.0" layoutX="96.0" layoutY="86.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="2.0" width="408.0" />
      <GridPane layoutX="163.0" layoutY="172.0" prefHeight="101.0" prefWidth="280.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="165.0" minWidth="10.0" prefWidth="104.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="195.0" minWidth="10.0" prefWidth="194.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="New email:">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <TextField fx:id="newEmailFld" prefHeight="25.0" prefWidth="180.0" promptText="New Email" GridPane.columnIndex="1" />
            <PasswordField fx:id="passwordFld" promptText="Current password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" GridPane.rowIndex="1">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
         </children>
      </GridPane>
      <Button fx:id="DoneBtn" layoutX="391.0" layoutY="261.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Done" />
      <Button fx:id="backBtn" layoutX="336.0" layoutY="261.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Back" />
      <Label fx:id="error" layoutX="195.0" layoutY="106.0" textFill="#b91010">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="message" layoutX="251.0" layoutY="106.0" textFill="#2fa84f">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Text layoutX="163.0" layoutY="158.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Current email:">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Label fx:id="currentEmailLabel" layoutX="264.0" layoutY="141.0">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
